{
  "name": "client",
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "check:format": "npm run prettier -- --list-different",
    "format": "npm run prettier -- --write",
    "lint": "eslint --ext .js,.jsx ./src",
    "prettier": "prettier \"**/*.+(js|json|css|scss|md|graphql)\"",
    "setup": "npm install && npm run validate",
    "start": "serve dist",
    "start:dev": "NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "test": "jest --watchAll",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watchAll",
    "test:coverage": "npm run test:once -- --coverage",
    "test:once": "jest",
    "validate": "npm run check:format && npm run lint"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "mini-css-extract-plugin": "^1.3.4",
    "msw": "^0.25.0",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "serve": "^11.3.2",
    "source-map-loader": "^2.0.0",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.2",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
