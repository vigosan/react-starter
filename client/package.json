{
  "name": "client",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --color",
    "check:format": "npm run prettier -- --list-different",
    "check:types": "tsc",
    "format": "npm run prettier -- --write",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "prettier": "prettier \"**/*.+(js|json|css|scss|ts|tsx|md|graphql)\"",
    "setup": "npm install && npm run validate",
    "start": "serve dist",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --color",
    "test": "jest --watchAll",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watchAll",
    "test:coverage": "npm run test:once -- --coverage",
    "test:once": "jest",
    "validate": "npm run check:types && npm run check:format && npm run lint"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^6.0.0-alpha.5",
    "react-router-dom": "^6.0.0-alpha.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-watch-typeahead": "^0.6.1",
    "mini-css-extract-plugin": "^0.11.3",
    "postcss-loader": "^4.0.3",
    "prettier": "^2.1.2",
    "serve": "^11.3.2",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.3.0",
    "tailwindcss": "^1.8.10",
    "terser-webpack-plugin": "^4.2.2",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
